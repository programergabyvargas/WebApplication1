@model WebApplication1.Models.Contrato

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>¿Estás segura de que quieres eliminar el Contrato?</h3>
<div>
    <h4>Contrato</h4>
    <hr />
    <dl class="row">
       
        
        <dt class = "col-sm-2">
            Fecha de Inicio:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaInicio)
        </dd>
        <dt class = "col-sm-2">
            Fecha de Finalización:
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaFin)
        </dd>
        <dt class = "col-sm-2">
           Monto Mensual:
        </dt>
        <dd class = "col-sm-10">
            $ @Html.DisplayFor(model => model.MontoMensual)
        </dd>
        <dt class = "col-sm-2">
            Inmueble: 
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PropiedadAlquilada.Direccion)
        </dd>
        <dt class = "col-sm-2">
            Inquilino:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Locatario.Apellido)
            @Html.DisplayFor(model => model.Locatario.Nombre)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver a la Lista</a>
    </form>
</div>
